#!/bin/bash
. /etc/profile
PATH=/opt/gvm/lib:$PATH
LOG="/tmp/$$.log"
err=0
#greenbone-nvt-sync > "$LOG" 2>&1
#test "$?" -ne "0" && err=1
/usr/sbin/greenbone-scapdata-sync >> "$LOG" 2>&1
test "$?" -ne "0" && err=1
/usr/sbin/greenbone-certdata-sync >> "$LOG" 2>&1
test "$?" -ne "0" && err=1
test "$err" = 1 && cat "$LOG"
rm -f "$LOG"
